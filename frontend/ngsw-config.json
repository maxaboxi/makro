{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js", "/**/*.json"]
      },
      "urls": ["https://fonts.googleapis.com/css?family=Roboto", "https://fonts.gstatic.com/**"]
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "backend",
      "urls": ["https://api.makro.diet/api/v2/"],
      "cacheConfig": {
        "maxSize": 50,
        "maxAge": "6h",
        "timeOut": "5s",
        "strategy": "freshness"
      }
    }
  ]
}
