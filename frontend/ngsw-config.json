{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js", "/**/*.json"]
      },
      "urls": ["https://fonts.googleapis.com/css?family=Roboto"]
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "foods",
      "urls": ["https://foods.makro.diet/api/v1/foods/getallfoods"],
      "cacheConfig": {
        "maxSize": 2,
        "maxAge": "12h",
        "timeOut": "5s",
        "strategy": "freshness"
      }
    },
    {
      "name": "userFoods",
      "urls": ["https://foods.makro.diet/api/v1/foods/getfoods/*"],
      "cacheConfig": {
        "maxSize": 5,
        "maxAge": "12h",
        "timeOut": "5s",
        "strategy": "freshness"
      }
    },
    {
      "name": "validate",
      "urls": ["https://auth.makro.diet/auth/validatetoken"],
      "cacheConfig": {
        "maxSize": 2,
        "maxAge": "168h",
        "timeOut": "1s",
        "strategy": "freshness"
      }
    },
    {
      "name": "userInfo",
      "urls": ["https://auth.makro.diet/auth/getuserinfo"],
      "cacheConfig": {
        "maxSize": 2,
        "maxAge": "12h",
        "timeOut": "5s",
        "strategy": "freshness"
      }
    },
    {
      "name": "sharedmeals",
      "urls": ["https://api.makro.diet/api/v1/sharedmeals/getallsharedmeals"],
      "cacheConfig": {
        "maxSize": 2,
        "maxAge": "6h",
        "timeOut": "5s",
        "strategy": "freshness"
      }
    }
  ]
}
